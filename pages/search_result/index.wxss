.home-page{
	font-size:30rpx;
	font-family:PingFangSC-Regular;
}
.home-top{
	width:100%;
}
.search-sec{
	padding:14rpx 0;
}
.search{
	margin:0 auto;
	width:618rpx;
	height:92rpx;
	background-size: 100% 100%;
	display: flex;
	align-items: center;
	justify-content: center;
}

.search .search-icon{
	width:32rpx;
	height:32rpx;
	background-size: 100% 100%;
}
.search .search-prompt{
	font-size:32rpx;
	font-family:PingFangSC-Regular;
	color:rgba(66,192,255,1);
	line-height:92rpx;
	margin-left:8rpx;
}
.anchor-li{
	display: flex;
	/*justify-content: center;*/
	align-items: center;
	padding:35rpx 20rpx;
	border-bottom:1rpx solid #eee;
}


.result-prompt{
	font-size: 26rpx;
	padding:20rpx;
	margin:0 auto;
	display: flex;
	align-items: center;
	justify-content:center; 
	margin:32rpx auto;
}

.result-prompt .horizontal-line{
	width:220rpx;
	height:2rpx;
	border-bottom:2rpx solid rgba(53,53,53,1);
}
.result-prompt .prompt-word{
	height:45rpx;
	font-size:32rpx;
	font-family:PingFangSC-Light;
	color:rgba(53,53,53,1);
	line-height:45rpx;
	margin:0 28rpx;
	white-space: nowrap;
}
