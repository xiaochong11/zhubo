.search-page{
	min-height: 100vh;
}
.search-top{
	display: flex;
	background:rgba(220,243,255,1);
	padding:16rpx;
	box-sizing: border-box;

}

.input_container{
	width:618rpx;
	height:92rpx;
	padding-left: 36rpx;
	box-sizing: border-box;
	background-size: 100% 100%;
	border:none;
	border-radius: none;
}
.input_container input{
	width:582rpx;
	height:92rpx;
	line-height: 92rpx;
	font-size:30rpx;
}
.search-delete{
	width:88rpx;
	height:92rpx;
	background-size: 100% 100%;
	margin-left: 12rpx;
	background-image: url('data:image/png;base64,');
}

.can-clear{
	background-image: url('https://upyun.lesson.bbwansha.com/dub/program/images/home/home_window_btn_close copy.png');
}

.search-body{
	padding:24rpx;
	box-sizing: border-box;
	min-height: calc(100vh - 62rpx);
	background-color:#ccecfe;
}

.search-body .title{
	font-size:24rpx;
	font-family:PingFangSC-Regular;
	color:rgba(27,161,228,1);
	margin-bottom:24rpx;
}
.search-body{
	
}
.search-record{
	display: flex;
	flex-wrap: wrap;
}
.search-body .search-li{
	height:56rpx;
	box-sizing: border-box;
	line-height:56rpx;
	padding:0 24rpx;
	background:rgba(255,255,255,1);
	border-radius:28px;
	font-size:24rpx;
	font-family:PingFangSC-Regular;
	color:rgba(53,53,53,1);
	margin-right:24rpx;
	margin-bottom:16rpx;
}
.search-body .search-li text{
	white-space: nowrap;
}







